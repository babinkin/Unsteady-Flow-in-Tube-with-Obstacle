FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (5 0 0);

boundaryField
{
    in
    {
        type            fixedValue;
        value           uniform (5 0 0);
    }

    out
    {
        type            zeroGradient;
    }

    sym
    {
        type            symmetryPlane;
    }
    
    wall1
    {
        type            noSlip;
    }
    
    wall2
    {
        type            noSlip;
    }
    
    wall3
    {
        type            noSlip;
    }
}

